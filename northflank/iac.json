                          "projectType": "github",
                          "accountLogin": "amiller68",
                          "projectUrl": "https://github.com/amiller68/iac-experiment"
                        },
                        "buildConfiguration": {
                          "prRestrictions": [
                            "feat/*"
                          ],
                          "branchRestrictions": [
                            "main"
                          ]
                        },
                        "buildSettings": {
                          "dockerfile": {
                            "buildEngine": "kaniko",
                            "useCache": false,
                            "dockerWorkDir": "/packages/database",
                            "dockerFilePath": "/packages/database/Dockerfile",
                            "buildkit": {
                              "useInternalCache": false,
                              "internalCacheStorage": 16384
                            }
                          }
                        },
                        "disabledCI": false
                      },
                      "ref": "iac-experiment-db-migrate-builder"
                    }
                  ],
                  "context": {
                    "projectId": "${refs.iac-experiment.id}"
                  }
                }
              },
              {
                "kind": "Addon",
                "spec": {
                  "externalAccessEnabled": false,
                  "type": "postgresql",
                  "billing": {
                    "replicas": 1,
                    "storage": 4096,
                    "storageClass": "ssd",
                    "deploymentPlan": "nf-compute-50"
                  },
                  "tlsEnabled": true,
                  "typeSpecificSettings": {
                    "postgresqlConnectionPoolerReplicas": 2,
                    "postgresqlReadConnectionPoolerReplicas": 2
                  },
                  "name": "Postgres",
                  "version": "16",
                  "projectId": "${refs.iac-experiment.id}"
                },
                "ref": "postgres"
              },
              {
                "kind": "SecretGroup",
                "spec": {
                  "type": "secret",
                  "secretType": "environment-arguments",
                  "priority": 10,
                  "secrets": {
                    "variables": {},
                    "files": {}
                  },
                  "addonDependencies": [
                    {
                      "addonId": "${refs.postgres.id}",
                      "keys": [
                        {
                          "keyName": "POSTGRES_URI",
                          "aliases": [
                            "DB_URI"
                          ]
                        }
                      ]
                    }
                  ],
                  "name": "SecretGroup",
                  "restrictions": {
                    "restricted": false,
                    "nfObjects": [],
                    "tags": []
                  },
                  "projectId": "${refs.iac-experiment.id}"
                },
                "ref": "secretgroup"
              },
              {
                "kind": "ManualJob",
                "spec": {
                  "runOnSourceChange": "always",
                  "backoffLimit": 0,
                  "activeDeadlineSeconds": 600,
                  "deployment": {
                    "storage": {
                      "ephemeralStorage": {
                        "storageSize": 1024
                      },
                      "shmSize": 64
                    },
                    "docker": {
                      "configType": "default"
                    },
                    "internal": {
                      "id": "${refs.iac-experiment-db-migrate-builder.id}",
                      "branch": "main",
                      "buildSHA": "latest"
                    }
                  },
                  "runtimeEnvironment": {},
                  "runtimeFiles": {},
                  "billing": {
                    "deploymentPlan": "nf-compute-10",
                    "buildPlan": "nf-compute-400-16"
                  },
                  "name": "db-migrate",
                  "buildArguments": {},
                  "buildFiles": {},
                  "buildSettings": {
                    "dockerfile": {
                      "buildEngine": "kaniko",
                      "useCache": false,
                      "dockerWorkDir": "/packages/database",
                      "dockerFilePath": "/packages/database/Dockerfile",
                      "buildkit": {
                        "useInternalCache": false,
                        "internalCacheStorage": 16384
                      }
                    }
                  },
                  "buildConfiguration": {
                    "pathIgnoreRules": [],
                    "isAllowList": false,
                    "ciIgnoreFlagsEnabled": false
                  }
                },
                "ref": "db-migrate"
              },
              {
                "kind": "DeploymentService",
                "spec": {
                  "deployment": {
                    "instances": 1,
                    "storage": {
                      "ephemeralStorage": {
                        "storageSize": 1024
                      },
                      "shmSize": 64

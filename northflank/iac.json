                        },
                        "name": "iac-experiment-api-service-builder",
                        "vcsData": {
                          "projectType": "github",
                          "accountLogin": "amiller68",
                          "projectUrl": "https://github.com/amiller68/iac-experiment"
                        },
                        "buildConfiguration": {
                          "prRestrictions": [],
                          "branchRestrictions": [
                            "main"

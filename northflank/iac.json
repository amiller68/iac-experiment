                        "buildSettings": {
                          "dockerfile": {
                            "buildEngine": "kaniko",
                            "useCache": false,
                            "dockerWorkDir": "/apps/web-service",
                            "dockerFilePath": "/apps/web-service/Dockerfile",
                            "buildkit": {
                              "useInternalCache": false,
                              "internalCacheStorage": 16384
                            }
                          }
                        },
                        "disabledCI": false
                      },
                      "ref": "iac-experiment-web-service-builder"
